    "express": "^4.16.2",
    "express-bearer-token": "^2.1.0",
    "express-jwt": "^5.3.0",
    "express-winston": "^2.4.0",
    "fabric-ca-client": "^1.1.0-snapshot.18",
    "fabric-client": "^1.1.0-snapshot.18",
    "fs-extra": "^4.0.2",
    "http-status": "^1.0.1",
    "inversify": "^4.3.0",
    "inversify-express-utils": "^4.0.1",
    "joi": "^13.0.1",
    "jsonwebtoken": "^8.1.0",
    "nats": "^0.7.24",
    "reflect-metadata": "^0.1.10",
    "strong-error-handler": "^2.3.0",
    "web-request": "^1.0.7",
    "winston": "^2.4.0"

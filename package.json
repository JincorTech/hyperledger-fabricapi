{
  "name": "fabricapi",
  "version": "0.0.1",
  "private": true,
  "description": "Fabric API is a service client for Hyperledger Fabric",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -w ./src -e ts ./src/server --exec ts-node",
    "lint": "tslint --fix -c tslint.json ./src/**/**/*.ts",
    "build": "tsc --outDir dist ",
    "serve": "NODE_ENV=production node ./dist/server.js"
  },
  "engines": {
    "node": ">=8.8"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",

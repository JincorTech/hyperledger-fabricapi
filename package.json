  "devDependencies": {
    "@types/express": "^4.0.37",
    "@types/http-status": "^0.2.30",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/node": "^8.0.46",
    "@types/winston": "^2.3.6",
    "nodemon": "^1.12.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "tslint-config-standard": "^6.0.1",
    "typescript": "^2.5.3"
  }
